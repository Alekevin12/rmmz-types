{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "lib": ["ES6", "ES5", "ES2015.Iterable", "DOM", "DOM.Iterable"],
    "skipLibCheck": false,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "declaration": true
  },
  "include": ["lib"]
}
